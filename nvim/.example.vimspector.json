{
  "configurations": {
    "attach": {
      "adapter": "vscode-node",
      "default": true,
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      }
    },
    "configuration": {
      "name": "Attach To Process",
      "type": "none",
      "request": "attach",
      "skipFiles": ["<node_modules>/**/*.js"],
      "processId": "${processId}"
    }
  }
}
